'.text.html':
  'Dropdown':
    'prefix': '_vdd'
    'body': """
      <!-- ${1:description} -->
      <div class="row">
        <div class="col-md-12 form-group">
            <label for="${2:for}">${3:label}</label>
            <select ui-select2 id="${2:for}" ng-model="${4:model}" placeholder="${5:placeholder}">
                <option></option>
            </select>
        </div>
      </div>
      """
